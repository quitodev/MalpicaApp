                android:textColor="@color/colorText"
                android:textSize="18sp"
                tools:ignore="Autofill"/>

            <TextView
                android:id="@+id/textPrecioTotal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/editImpuestos"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_marginStart="50dp"
                android:layout_marginLeft="50dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="0dp"
                android:layout_marginRight="0dp"
                android:fontFamily="sans-serif-light"
                android:text="@string/precio_total"
                android:textColor="@color/colorAccent"
                android:textSize="16sp"/>

            <EditText
                android:id="@+id/editPrecioTotal"
                android:layout_width="310dp"
                android:layout_height="35dp"
                android:layout_below="@+id/textPrecioTotal"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_marginStart="50dp"
                android:layout_marginLeft="50dp"
                android:layout_marginEnd="51dp"
                android:layout_marginRight="51dp"
                android:background="#00000000"
                android:drawablePadding="5dp"
                android:focusable="false"
                android:fontFamily="sans-serif-light"
                android:hint="@string/muestra_precio_total"
                android:inputType="numberDecimal"
                android:maxLength="10"
                android:text=""
                android:textColorHint="@color/colorTextHint"
                android:textColor="@color/colorText"
                android:textSize="18sp"
                tools:ignore="Autofill"/>

            <Button
                android:id="@+id/buttonGuardar"
                android:layout_width="150dp"
                android:layout_height="35dp"
                android:layout_below="@+id/editPrecioTotal"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="40dp"
                android:background="#323232"
                android:drawableLeft="@drawable/ic_save_orange"
                android:fontFamily="sans-serif-light"
                android:paddingLeft="15dp"
                android:text="@string/guardar_datos"
                android:textColor="@color/colorAccent"
                android:textSize="16sp"
                tools:ignore="RtlHardcoded,RtlSymmetry"/>

            <Button
                android:id="@+id/buttonModificar"
                android:layout_width="150dp"
                android:layout_height="35dp"
                android:layout_below="@+id/editPrecioTotal"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="40dp"
                android:background="#323232"
                android:drawableLeft="@drawable/ic_edit_orange"
                android:fontFamily="sans-serif-light"
                android:paddingLeft="15dp"
                android:text="@string/modificar_datos"
                android:textColor="@color/colorAccent"
                android:textSize="16sp"
                tools:ignore="RtlHardcoded,RtlSymmetry"/>

            <EditText
                android:id="@+id/editVacio"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_below="@+id/buttonModificar"
                android:layout_marginTop="35dp"
                android:background="#00000000"
                android:focusable="false"
                android:gravity="center_horizontal"
                android:hint="@string/componentes"
                android:inputType="textPersonName"
                android:text=""
                tools:ignore="Autofill"/>

        </RelativeLayout>
    </ScrollView>
</RelativeLayout>